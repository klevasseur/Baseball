Cell[BoxData[
 RowBox[{
  RowBox[{"LongestStreak", "[", 
   RowBox[{"player_", ",", "gamelist_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"HitSafelyQ", "[", 
          RowBox[{"player", ",", "#"}], "]"}], "&"}], "/@", "gamelist"}], "//", 
       RowBox[{
        RowBox[{"Select", "[", 
         RowBox[{"#", ",", 
          RowBox[{
           RowBox[{"#", "=!=", "\"\<DNP\>\""}], "&"}]}], "]"}], "&"}]}], "//", 
      RowBox[{
       RowBox[{"Split", "[", "#", "]"}], "&"}]}], "//", 
     RowBox[{
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"First", "[", "#", "]"}], ",", 
           RowBox[{"Length", "[", "#", "]"}]}], "}"}], "&"}], ",", "#"}], 
       "]"}], "&"}]}], "//", 
    RowBox[{
     RowBox[{"Select", "[", 
      RowBox[{"#", ",", 
       RowBox[{
        RowBox[{"First", "[", "#", "]"}], "&"}]}], "]"}], "&"}]}], "//", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"#", "\[Equal]", 
       RowBox[{"{", "}"}]}], ",", "0", ",", 
      RowBox[{
       RowBox[{
        RowBox[{"Sort", "[", "#", "]"}], "//", "Last"}], "//", "Last"}]}], 
     "]"}], "&"}]}]}]], "Input",

Cell[BoxData[
 RowBox[{
  RowBox[{"StolenBases", "[", "g_retrogame", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"sbl", ",", "v", ",", "h"}], "}"}], ",", 
    RowBox[{
     RowBox[{"v", "=", 
      RowBox[{"VisitingTeam", "[", "g", "]"}]}], ";", 
     RowBox[{"h", "=", 
      RowBox[{"HomeTeam", "[", "g", "]"}]}], ";", 
     RowBox[{"sbl", "=", 
      RowBox[{"List", "@@", 
       RowBox[{"Select", "[", 
        RowBox[{"g", ",", 
         RowBox[{
          RowBox[{
           RowBox[{"Result", "[", "#", "]"}], "\[Equal]", "\"\<SB\>\""}], 
          "&"}]}], "]"}]}]}], ";", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"sbl", "\[Equal]", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"{", "}"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"List", "@@", "#"}], "&"}], "/@", "sbl"}], ")"}], "//", 
          "Transpose"}], "//", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "/.", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"0", "\[Rule]", "v"}], ",", 
               RowBox[{"1", "\[Rule]", "h"}]}], "}"}]}], ",", 
            RowBox[{"#", "[", 
             RowBox[{"[", "3", "]"}], "]"}], ",", 
            RowBox[{"#", "[", 
             RowBox[{"[", "6", "]"}], "]"}]}], "}"}], "&"}]}], "//", 
        "Transpose"}]}], "]"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.41605566571373*^9, 3.416055776656476*^9}, {
   3.4160558159937353`*^9, 3.416055848144134*^9}, {3.416055931318568*^9, 
